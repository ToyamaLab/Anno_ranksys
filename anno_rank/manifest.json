{
    "manifest_version":2,
    "name": "anno_rank",
    "version": "0.0.1",
    "background": {
        "scripts":["background.js"]
    },
    "content_scripts":[{
        "matches":["<all_urls>"],
        "js":["jquery.min.js","content.js"]
    }],
    "browser_action":{
        "default_popup":"popup.html"
    },
    "permissions":[
        "contextMenus",
        "http://*/*",
        "https://*/*",
        "tabs"
    ]
}